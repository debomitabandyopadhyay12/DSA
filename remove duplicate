
#include <iostream>
using namespace std;
void print_arr(int arr[],int n)
{
    cout<<endl;
    for(int i=0;i<n;i++)
    {
        cout<<arr[i]<<" ";
    }
    cout<<endl;
}
int remove_duplicate(int arr[],int n)
{
    int i=0;
    for(int j=1;j<n;j++)
    {
        if(arr[j]!=arr[i])
        {
            i++;
            arr[i]=arr[j];
        }
    }
    return i+1;
}

int main() {
 int i,n,arr[10];
 cout<<"enter th enumber of element";
 cin>>n;
 cout<<"enter the array element-";
 for(i=0;i<n;i++)
 {
     cin>>arr[i];
 }
 cout<<endl;
 print_arr(arr,n);
 
  cout<<endl;
 int ans=remove_duplicate(arr,n);
 print_arr(arr,n);
 
 cout<<endl;
 cout<<ans;
 
    return 0;
}
