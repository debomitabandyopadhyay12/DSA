// Online C++ compiler to run C++ program online
#include <iostream>
using namespace std;
void print_arr(int arr[],int n)
{
    cout<<endl;
    for(int i=0;i<n;i++)
    {
        cout<<arr[i]<<" ";
    }
    cout<<endl;
}
void rotateArray(int arr[], int n) {
    int last=arr[0];
    for(int i=1;i<=n;i++)
    {
        arr[i-1]=arr[i];
    }
    arr[n-1]=last;
    
    
}
int main() {
  int i,n,arr[10];
 cout<<"enter th enumber of element";
 cin>>n;
 cout<<"enter the array element-";
 for(i=0;i<n;i++)
 {
     cin>>arr[i];
 }
 print_arr(arr,n);
 rotateArray(arr,n);
  print_arr(arr,n);
    return 0;
}
